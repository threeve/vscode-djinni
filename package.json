{
    "name": "vscode-djinni",
    "displayName": "Djinni",
    "description": "Djinni extension for Visual Studio Code.",
    "version": "0.2.0",
    "publisher": "jafo",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages", "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "djinni",
            "aliases": ["Djinni", "djinni"],
            "extensions": ["djinni"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "djinni",
            "scopeName": "source.djinni",
            "path": "./syntaxes/djinni.json"
        }],
        "snippets": [{
            "language": "djinni",
            "path": "./snippets/djinni.json"
        }]
    }
}